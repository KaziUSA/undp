{% extends '::base.html.twig' %}

{% block body -%}
    <h1>IssueQuestion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Keyfindingsmonth</th>
                <td>{{ entity.keyFindingsMonth }}</td>
            </tr>
            <tr>
                <th>Keyfindings</th>
                <td>{{ entity.keyFindings }}</td>
            </tr>
            <tr>
                <th>Infographicstitle</th>
                <td>{{ entity.infographicsTitle }}</td>
            </tr>
            <tr>
                <th>Hrrp</th>
                <td>{{ entity.hrrp }}</td>
            </tr>
            <tr>
                <th>Districtid</th>
                <td>{{ entity.districtId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('issuequestion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('issuequestion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
